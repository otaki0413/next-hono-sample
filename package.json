    "@hono/zod-openapi": "^1.1.4",
    "@mui/material": "^7.3.2",
    "@mui/material-nextjs": "^7.3.2",
    "@prisma/client": "^6.16.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "better-auth": "^1.3.18",
    "hono": "^4.10.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "lefthook": "^1.13.1",
    "prisma": "^6.16.2",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "lefthook",
      "prisma",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
